    "@types/styled-components": "5.1.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.0",
    "nextjs-sitemap-generator": "1.0.0",
