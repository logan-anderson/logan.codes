    "@headlessui/react": "0.3.2",
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@tailwindcss/ui": "0.6.2",
    "@tinacms/auth": "^1.0.11",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.3.2",
    "clsx": "^2.1.1",
    "dotenv": "8.6.0",
    "fast-glob": "3.2.2",
    "gray-matter": "4.0.2",
