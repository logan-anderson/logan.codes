{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "tinacms dev -c \"next dev\"",
    "build": "tinacms build && pnpm build:app",
    "export": "build && next export",
    "build:app": "pnpm build:rss && next build",
    "analyze": "cross-env ANALYZE=true next build",
    "build:rss": "tsc --project tsconfig.rss.json && node scripts/generate-rss.js",
    "start": "next start",
    "type-check": "tsc",
    "lint": "next lint",
    "lighthouse": "docker run --rm -v $PWD/test_results:/home/chrome/reports emazzotta/lighthouse-badges /bin/sh -c \"lighthouse-badges --urls https:logan.codes\""
  },
