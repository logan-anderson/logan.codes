    "next": "^15.2.1",
    "next-seo": "4.5.0",
    "next-themes": "^0.3.0",
    "next-tinacms-cloudinary": "^13.0.2",
    "posthog-js": "^1.141.3",
    "react": "^19.0.0",
    "react-awesome-reveal": "3.8.1",
    "react-collapsible": "2.8.0",
    "react-dom": "^19.0.0",
    "react-markdown": "4.3.1",
