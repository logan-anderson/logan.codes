{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "tinacms dev -c \"next dev\"",
    "build": "tinacms build && yarn build:app",
    "export": "build && next export",
    "build:app": "yarn build:rss && next build",
    "analyze": "cross-env ANALYZE=true next build",
    "build:rss": "tsc --project tsconfig.rss.json && node scripts/generate-rss.js",
    "start": "next start",
    "type-check": "tsc",
    "lint": "next lint",
    "lighthouse": "docker run --rm -v $PWD/test_results:/home/chrome/reports emazzotta/lighthouse-badges /bin/sh -c \"lighthouse-badges --urls https:logana.dev\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.15",
    "@headlessui/react": "0.3.2",
    "@tailwindcss/aspect-ratio": "0.2.1",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "0.4.1",
    "@tailwindcss/ui": "0.6.2",
    "@tinacms/auth": "^1.0.0",
    "dotenv": "8.6.0",
    "fast-glob": "3.2.2",
    "gray-matter": "4.0.2",
    "gsap": "3.7.1",
    "next": "11.1.0",
    "next-pwa": "5.3.1",
    "next-seo": "4.5.0",
    "next-tinacms-cloudinary": "^4.0.0",
    "postcss": "8.3.6",
    "raw-loader": "4.0.1",
    "react": "17.0.2",
    "react-awesome-reveal": "3.8.1",
    "react-collapsible": "2.8.0",
    "react-dom": "17.0.2",
    "react-markdown": "4.3.1",
    "react-syntax-highlighter": "15.4.4",
    "rss": "1.2.2",
    "styled-components": "5.1.1",
    "tinacms": "^1.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "10.2.0",
    "@tinacms/cli": "^1.0.0",
    "@types/classnames": "2.2.10",
    "@types/gsap": "1.20.2",
    "@types/gtag.js": "0.0.7",
    "@types/js-yaml": "3.12.4",
    "@types/node": "14.17.12",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/react-syntax-highlighter": "13.5.2",
    "@types/rss": "0.0.28",
    "@types/styled-components": "5.1.0",
    "cross-env": "^7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.0",
    "nextjs-sitemap-generator": "1.0.0",
    "postcss-preset-env": "6.7.0",
    "tailwindcss": "^2.0.1",
    "typescript": "4.3.5"
  },
